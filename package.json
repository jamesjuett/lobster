        "chai": "^4.3.4",
        "css-loader": "^3.2.0",
        "eslint": "^7.1.0",
        "eslint-cli": "^1.1.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-formatter-pretty": "^4.1.0",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-prettier": "^3.4.0",
